{
    "dependencies": {
        "@grpc/grpc-js": "^1.8.4",
        "@taquito/michelson-encoder": "^16.1.2",
        "@taquito/taquito": "^16.1.2",
        "@tezos-dappetizer/cli": "^2.7.1",
        "@tezos-dappetizer/database": "^2.7.1",
        "@tezos-dappetizer/decorators": "^2.7.1",
        "@tezos-dappetizer/indexer": "^2.7.1",
        "@tezos-dappetizer/utils": "^2.7.1",
        "@types/google-protobuf": "^3.15.6",
        "bignumber.js": "^9.1.1",
        "sqlite3": "^5.1.6",
        "typeorm": "^0.3.15"
    },
    "devDependencies": {
        "grpc_tools_node_protoc_ts": "^5.3.3",
        "grpc-tools": "^1.12.4",
        "rimraf": "^5.0.0",
        "ts-protoc-gen": "^0.15.0",
        "typescript": "^4.9.5"
    },
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build": "tsc -p ./tsconfig.json",
        "build-grpc": "grpc_tools_node_protoc --plugin='protoc-gen-ts=./node_modules/.bin/protoc-gen-ts' --plugin='protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin' --js_out='import_style=commonjs,binary:./src' --ts_out='grpc_js:./src' --grpc_out='grpc_js:./src' --proto_path='../../protos/' event-processor.proto"
    }
